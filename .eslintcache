s/${id}?embed[]=seasons&embed[]=cast`).then(results => {\r\n        if(isMounted){\r\n          dispatch({type: 'FETCH_SUCCESS',show : results})\r\n        }\r\n    }).catch(err =>{\r\n      if(isMounted) {\r\n        dispatch({type: 'FETCH_FAILED',error : err.message})\r\n      }\r\n    });\r\n    return () => {\r\n      isMounted = false;\r\n    }\r\n  },[id]);\r\n  console.log('show',show);\r\n \r\n\r\n  if(isLoading){\r\n    return <div>data is being loading</div>;\r\n  }\r\n\r\n  if(error){\r\n    return <div> error message : {error}</div>;\r\n  }\r\n\r\n  return (\r\n    <ShowPageWrapper>\r\n      <ShowMainData \r\n      image = {show.image} \r\n      name = {show.name} \r\n      rating ={show.rating} \r\n      summary = {show.summary} \r\n      tags = {show.genres} />\r\n      <InfoBlock>\r\n        <h2> Details </h2>\r\n        <Details status = {show.status} network = {show.network} premiered = {show.premiered} />\r\n      </InfoBlock>\r\n\r\n      <InfoBlock>\r\n        <h2> Seasons </h2>\r\n        <Seasons seasons = {show._embedded.seasons} />\r\n      </InfoBlock>\r\n\r\n      <InfoBlock>\r\n        <h2> Cast </h2>\r\n        <Cast cast = {show._embedded.cast} />\r\n      </InfoBlock>\r\n    </ShowPageWrapper>\r\n  );\r\n};\r\n\r\nexport default Show\r\n","C:\\Users\\nages\\Desktop\\project\\box-office\\src\\components\\show\\Cast.js",[],"C:\\Users\\nages\\Desktop\\project\\box-office\\src\\components\\show\\Details.js",[],"C:\\Users\\nages\\Desktop\\project\\box-office\\src\\components\\show\\ShowMainData.js",[],"C:\\Users\\nages\\Desktop\\project\\box-office\\src\\components\\show\\Seasons.js",[],"C:\\Users\\nages\\Desktop\\project\\box-office\\src\\pages\\Show.Styled.js",[],"C:\\Users\\nages\\Desktop\\project\\box-office\\src\\components\\show\\Seasons.Styled.js",[],"C:\\Users\\nages\\Desktop\\project\\box-office\\src\\components\\show\\ShowMainData.Styled.js",[],"C:\\Users\\nages\\Desktop\\project\\box-office\\src\\components\\show\\Cast.Styled.js",